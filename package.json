{
  "name": "express-socket-json-routes",
  "version": "1.0.0",
  "description": "Allows for express.js and socket.io routes to be created using a signle JSON object",
  "main": "index.js",
  "scripts": {
    "test": "set NODE_ENV=test&& node ./node_modules/mocha/bin/mocha --R spec ./test/unit/*",
    "coverage": "set NODE_ENV=test&& node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/unit/*",
    "opencoverage": "npm run coverage && node ./test/assets/openCoverage.js"
  },
  "author": "Benjamin Caldwell",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.11.0",
    "ejs": "^2.3.4",
    "istanbul": "^0.4",
    "mocha": "^2.2.5",
    "open": "0.0.5",
    "q": "^1.4.1",
    "request": "^2.57.0",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.7",
    "supertest": "^1.1.0",
    "unit.js": "^2.0.0"
  },
  "dependencies": {
    "lodash": "^4",
    "debug": "^2.2.0",
    "express": "^4.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjamincaldwell/express-socket-json-routes.git"
  },
  "keywords": [
    "express",
    "express.js",
    "socket",
    "realtime",
    "socket.io",
    "json"
  ],
  "bugs": {
    "url": "https://github.com/benjamincaldwell/express-socket-json-routes/issues"
  },
  "homepage": "https://github.com/benjamincaldwell/express-socket-json-routes#readme"
